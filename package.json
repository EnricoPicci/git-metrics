{
  "name": "git-metrics",
  "version": "1.1.9",
  "description": "tools to calculate some metrics out of git repos",
  "main": "index.js",
  "bin": {
    "git-metrics": "./dist/3-lib/run-reports-on-repo-parallel-reads.js",
    "run-reports": "./dist/3-lib/run-reports-on-repo-parallel-reads.js",
    "run-reports-single-thread": "./dist/3-lib/run-reports-on-repo-single-thread.js",
    "run-branches-report": "./dist/3-lib/run-branches-report.js",
    "run-all-reports-on-merged-repos": "./dist/3-lib/run-reports-on-merged-repos.js",
    "run-all-reports-on-multi-repos": "./dist/3-lib/run-reports-on-multi-repos.js",
    "load-mongo-run-reports": "./dist/3-lib/launch-load-mongo-run-reports.js",
    "run-repo-coupling-report": "./dist/3-lib/run-repo-coupling-report.js"
  },
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts src/**/**/*.spec.ts",
    "test-all": "mocha -r ts-node/register src/**/*.spec.ts src/**/**/*.spec.ts src/**/*.spec-mongo.ts src/**/**/*.spec-mongo.ts",
    "tsc": "tsc",
    "tsc-version": "tsc --version",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm version patch",
    "preversion": "npm run test",
    "version": "npm run tsc && git add -A dist",
    "postversion": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnricoPicci/git-metrics.git"
  },
  "keywords": [
    "git",
    "metrics"
  ],
  "author": "Picci",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EnricoPicci/git-metrics/issues"
  },
  "homepage": "https://github.com/EnricoPicci/git-metrics#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "observable-fs": "^2.2.4",
    "observable-mongo": "^2.3.16",
    "rxjs": "^7.4.0",
    "xlsx": "file:vendor/xlsx-0.19.3.tgz"
  }
}
